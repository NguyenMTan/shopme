@model List<Brand>

@{
    ViewData["Title"] = "List of brands";
}

<main>
    <div class="container-brand">
        <h2 class="title">Danh sách thương hiệu</h2>
        <div class="list-brand">
            <div class="item-brand">
                <div class="bg-logo">
                    <img src="data:image/png;base64,"
                         alt=""
                         class="logo" />
                </div>
                <h3 class="name">Name</h3>
                <div class="action">
                    <a href="" class="act act-edit">Edit</a>
                    <a href="" class="act act-details">Details</a>
                    <a href="" class="act act-delete">Delete</a>
                </div>
            </div>
            <div class="item-brand">
                <div class="bg-logo">
                    <img src="data:image/png;base64,"
                         alt=""
                         class="logo" />
                </div>
                <h3 class="name">Name</h3>
                <div class="action">
                    <a href="" class="act act-edit">Edit</a>
                    <a href="" class="act act-details">Details</a>
                    <a href="" class="act act-delete">Delete</a>
                </div>
            </div>
        </div>
    </div>
</main>

@section css {
    <link rel="stylesheet" href="./css/style-brand.css" />
}